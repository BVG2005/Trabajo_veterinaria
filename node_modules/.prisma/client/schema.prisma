datasource mysql {
  provider = "mysql"
  url      = env("MYSQL_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Registro {
  id       Int    @id @default(autoincrement())
  nombre   String
  apellido String
  cuidad   String
  email    String
  telefono String
}

model Usuarios {
  id       Int    @id @default(autoincrement())
  usuario  String
  password String
}
